{
    "name": "pro-deployer",
    "displayName": "PRO Deployer",
    "description": "PRO Deployer - Simple and powerful SFTP/FTP deployer",
    "version": "0.9.2",
    "publisher": "heminei",
    "categories": [
        "Other"
    ],
    "bugs": {
        "url": "https://github.com/heminei/vscode-pro-deployer/issues"
    },
    "repository": {
        "url": "https://github.com/heminei/vscode-pro-deployer"
    },
    "keywords": [
        "ftp",
        "sftp",
        "deploy",
        "remote",
        "multiple"
    ],
    "engines": {
        "vscode": "^1.53.0"
    },
    "activationEvents": [
        "onStartupFinished",
        "onCommand:pro-deployer.generate-config-file",
        "workspaceContains:.vscode/pro-deployer.json"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "pro-deployer.generate-config-file",
                "title": "Generate Config File",
                "category": "PRO Deployer"
            },
            {
                "command": "pro-deployer.upload",
                "title": "Upload",
                "category": "PRO Deployer"
            },
            {
                "command": "pro-deployer.upload-to",
                "title": "Upload To...",
                "category": "PRO Deployer"
            },
            {
                "command": "pro-deployer.upload-all-open",
                "title": "Upload All Open Files To...",
                "category": "PRO Deployer"
            }
        ],
        "keybindings": [
            {
                "command": "pro-deployer.upload",
                "key": "alt+u",
                "mac": "alt+u"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "pro-deployer.upload",
                    "group": "pro-deployer"
                },
                {
                    "command": "pro-deployer.upload-to",
                    "group": "pro-deployer"
                }
            ],
            "explorer/context": [
                {
                    "command": "pro-deployer.upload",
                    "group": "pro-deployer"
                },
                {
                    "command": "pro-deployer.upload-to",
                    "group": "pro-deployer"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "dependencies": {
        "ftp": "^0.3.10",
        "gitignore-parser": "0.0.2",
        "micromatch": "^4.0.4",
        "path": "^0.12.7",
        "ssh2": "^0.8.9"
    },
    "devDependencies": {
        "@types/ftp": "^0.3.31",
        "@types/gitignore-parser": "0.0.0",
        "@types/glob": "^7.1.3",
        "@types/micromatch": "^4.0.2",
        "@types/mocha": "^8.0.4",
        "@types/node": "^12.20.1",
        "@types/ssh2": "^0.5.47",
        "@types/vscode": "^1.53.0",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "esbuild": "^0.12.20",
        "eslint": "^7.20.0",
        "glob": "^7.1.6",
        "mocha": "^8.3.0",
        "typescript": "^4.1.5",
        "vscode-test": "^1.5.1"
    }
}
